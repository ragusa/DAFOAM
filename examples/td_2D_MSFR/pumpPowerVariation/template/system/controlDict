// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
FoamFile
{
    version         2;
    format          ascii;
    class           dictionary;
    object          controlDict;
}

application     GeN-Foam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         20;

deltaT          0.001;

writeControl    adjustableRunTime;

writeInterval   1;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   8;

runTimeModifiable yes;

regionSolvers
{
    energyNeutroLoop multiPhysicsSolver;
}

removeBaffles
{
    fluidRegion     false;
    neutroRegion    false;
}

liquidFuel      true;

adjustTimeStep  true;

maxDeltaT       1;

maxCo           5;

maxPowerVariation 0.025;


