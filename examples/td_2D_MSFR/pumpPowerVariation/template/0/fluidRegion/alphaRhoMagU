FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0/fluidRegion";
    object      alphaRhoMagU;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    front
    {
        type            wedge;
    }
    back
    {
        type            wedge;
    }
    bottomwall
    {
        type            calculated;
        value           uniform 0;
    }
    topwall
    {
        type            calculated;
        value           uniform 0;
    }
    reflector
    {
        type            calculated;
        value           uniform 0;
    }
    hx
    {
        type            calculated;
        value           uniform 0;
    }
}


